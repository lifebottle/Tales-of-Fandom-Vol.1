
<SceneText>
  <FriendlyName>Conversation_0085</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x71c55</PointerOffset>
      <JapaneseText>もう。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71c5c</PointerOffset>
      <JapaneseText>お兄ちゃんたら、どうしてそうかなあ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71c82</PointerOffset>
      <JapaneseText>クレスへの態度を批判する</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71c9c</PointerOffset>
      <JapaneseText>クレスを弁護する</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71cae</PointerOffset>
      <JapaneseText>皆と仲良くしてと言う</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71cc4</PointerOffset>
      <JapaneseText>昼間の事だって、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71cd5</PointerOffset>
      <JapaneseText>クレスさんに、あんな言いかたするのって、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71cfe</PointerOffset>
      <JapaneseText>よくないと思うよ？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71d11</PointerOffset>
      <JapaneseText>何の苦労も知らねえで育ったような</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71d32</PointerOffset>
      <JapaneseText>ボンボンに、同情されて喜べってのか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71d57</PointerOffset>
      <JapaneseText>いいか、アミィ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71d68</PointerOffset>
      <JapaneseText>ああいう奴は、他人に同情することで、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71d8d</PointerOffset>
      <JapaneseText>いい事をした、って満足してるだけだ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71db2</PointerOffset>
      <JapaneseText>俺たちは、可哀相なやつだ、って</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71dd1</PointerOffset>
      <JapaneseText>馬鹿にされてんだ。わかったな？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71df0</PointerOffset>
      <JapaneseText>わかったら、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71dfd</PointerOffset>
      <JapaneseText>あんな奴となれなれしくすんじゃねえぞ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71e25</PointerOffset>
      <JapaneseText>……なによ、お兄ちゃんのばか。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71e44</PointerOffset>
      <JapaneseText>クレスさんは、そんなんじゃないんだから。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71e6e</PointerOffset>
      <JapaneseText>クレスさんのことだって、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71e87</PointerOffset>
      <JapaneseText>何であんな態度取るの？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71e9e</PointerOffset>
      <JapaneseText>クレスさん、いい人だよ？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71eb7</PointerOffset>
      <JapaneseText>ハッ！　どうだかな。いい人面して、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71eda</PointerOffset>
      <JapaneseText>俺たちに近づいてきた連中を忘れたのか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71f01</PointerOffset>
      <JapaneseText>どいつもこいつも、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71f14</PointerOffset>
      <JapaneseText>ハイエナ以下の奴らだったろうが。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71f35</PointerOffset>
      <JapaneseText>そうだけど……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71f47</PointerOffset>
      <JapaneseText>……でも、クレスさんは違うと思う。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71f6b</PointerOffset>
      <JapaneseText>クレスさんのこともそうだけど、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71f8a</PointerOffset>
      <JapaneseText>もっとみんなと仲良くした方がいいよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71faf</PointerOffset>
      <JapaneseText>あたしたち、ここで暮らしていくんだから。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71fd8</PointerOffset>
      <JapaneseText>こびろってのか？　ごめんだね。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x71ff7</PointerOffset>
      <JapaneseText>お兄ちゃん……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x72009</PointerOffset>
      <JapaneseText>このままじゃ、いけないよ……やっぱり。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x72031</PointerOffset>
      <JapaneseText>何してんだ、早く来いよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
