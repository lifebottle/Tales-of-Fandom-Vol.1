
<SceneText>
  <FriendlyName>Conversation_0154</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x9e312</PointerOffset>
      <JapaneseText>村がユークリッド王家に納める税金を積んだ</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x9e33b</PointerOffset>
      <JapaneseText>馬車が襲われ、たくさんの人が死んだ……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x9e364</PointerOffset>
      <JapaneseText>でも……お兄ちゃんが、その仲間だった</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x9e389</PointerOffset>
      <JapaneseText>なんて話は、あたしは信じない。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x9e3a8</PointerOffset>
      <JapaneseText>必ずお兄ちゃんを見つけ出して、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x9e3c7</PointerOffset>
      <JapaneseText>真実を突き止める。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x9e3da</PointerOffset>
      <JapaneseText>たとえ、クレスさんが追ってくるのだと</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
