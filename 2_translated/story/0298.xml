
<SceneText>
  <FriendlyName>Conversation_0298</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0xee24e</PointerOffset>
      <JapaneseText>そうだ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee257</PointerOffset>
      <JapaneseText>チェスターにも参加してもらえないかな？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee27e</PointerOffset>
      <JapaneseText>アルベイン流は剣士ばかりだから、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee29f</PointerOffset>
      <JapaneseText>弓使いが敵にいたときのために、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee2be</PointerOffset>
      <JapaneseText>手伝ってもらえるとありがたいんだけどな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee2e7</PointerOffset>
      <JapaneseText>……お兄ちゃん、いません。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee302</PointerOffset>
      <JapaneseText>ゴーリさんとこの狩猟隊に参加してるの？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee329</PointerOffset>
      <JapaneseText>狩猟隊は、来週までおやすみです……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee34f</PointerOffset>
      <JapaneseText>どうしたの、アミィちゃん？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee36a</PointerOffset>
      <JapaneseText>なにかあったの？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee37c</PointerOffset>
      <JapaneseText>何でもありません。平気です。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee399</PointerOffset>
      <JapaneseText>……クレスさん、頑張ってくださいね！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee3be</PointerOffset>
      <JapaneseText>あ、うん……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee3ce</PointerOffset>
      <JapaneseText>……お兄ちゃん、様子が変なんです。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee3f1</PointerOffset>
      <JapaneseText>チェスターが？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee400</PointerOffset>
      <JapaneseText>パパとママの命日が近いのに、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee41d</PointerOffset>
      <JapaneseText>なんだか上の空で、それなのに、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee43c</PointerOffset>
      <JapaneseText>何を聞いても「心配するな」って</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee45b</PointerOffset>
      <JapaneseText>いうばっかりで……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee470</PointerOffset>
      <JapaneseText>ふうん……わかった。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee485</PointerOffset>
      <JapaneseText>チェスターにあったら、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee49c</PointerOffset>
      <JapaneseText>僕がそれとなく聞いてみるよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee4b9</PointerOffset>
      <JapaneseText>……いいんですか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee4cc</PointerOffset>
      <JapaneseText>大丈夫、任せておいて。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee4e4</PointerOffset>
      <JapaneseText>……お兄ちゃん、様子が変なんです。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee507</PointerOffset>
      <JapaneseText>チェスターが？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee516</PointerOffset>
      <JapaneseText>パパとママの命日が近いのに、なんだか上の空で、それなのに、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee551</PointerOffset>
      <JapaneseText>何を聞いても「心配するな」って</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0xee570</PointerOffset>
      <JapaneseText>いうばっかりで……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
