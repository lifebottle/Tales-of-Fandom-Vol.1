
<SceneText>
  <FriendlyName>Conversation_0330</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x1091c5</PointerOffset>
      <JapaneseText>立ちたくて、一人で森に見回りに来て、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1091ea</PointerOffset>
      <JapaneseText>それで、こいつらを見つけたんだよね。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x10920f</PointerOffset>
      <JapaneseText>そうだろ？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x10921a</PointerOffset>
      <JapaneseText>なん、だと……？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x10922b</PointerOffset>
      <JapaneseText>そうなんだろ、チェスター？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109246</PointerOffset>
      <JapaneseText>ちっ……ふざけんな！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x10925b</PointerOffset>
      <JapaneseText>俺は、てめえみたいな、いい子ちゃんが</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109280</PointerOffset>
      <JapaneseText>一番むかつくんだよ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109295</PointerOffset>
      <JapaneseText>クレイトンさん！　こいつ、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1092b0</PointerOffset>
      <JapaneseText>アルベイン道場の奴だ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1092c7</PointerOffset>
      <JapaneseText>チェスター！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1092d4</PointerOffset>
      <JapaneseText>なんだと？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1092df</PointerOffset>
      <JapaneseText>なら……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1092ea</PointerOffset>
      <JapaneseText>生かしちゃおけねえなっ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109303</PointerOffset>
      <JapaneseText>くそ……ガキがっ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109318</PointerOffset>
      <JapaneseText>悪人どもめ！　アルベイン流は田舎剣術</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x10933d</PointerOffset>
      <JapaneseText>なんかじゃないことを、教えてやるっ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109362</PointerOffset>
      <JapaneseText>くらえっ！　魔神剣っ！！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x10937d</PointerOffset>
      <JapaneseText>ぎゃあああっ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x10938c</PointerOffset>
      <JapaneseText>な、なんだこのガキはっ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1093a5</PointerOffset>
      <JapaneseText>つ、強えぞ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1093b2</PointerOffset>
      <JapaneseText>に、逃げろ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1093c1</PointerOffset>
      <JapaneseText>逃がすかっ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1093cf</PointerOffset>
      <JapaneseText>ぎゃーーーーー！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1093e0</PointerOffset>
      <JapaneseText>ひーーーーー！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1093f1</PointerOffset>
      <JapaneseText>つきあってらんねえぜ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109409</PointerOffset>
      <JapaneseText>逃がすかっ、チェスター！　裏切り者っ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109430</PointerOffset>
      <JapaneseText>――魔神剣っ！！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109442</PointerOffset>
      <JapaneseText>うわああああっ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109454</PointerOffset>
      <JapaneseText>はあはあはあはあ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109467</PointerOffset>
      <JapaneseText>……ちくしょう……ちくしょうーっ！！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x10948d</PointerOffset>
      <JapaneseText>馬鹿やろう！　俺のことは放っとけ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1094b0</PointerOffset>
      <JapaneseText>殺されるぞ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1094bd</PointerOffset>
      <JapaneseText>チェスター、アミィちゃんが心配してたよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1094e6</PointerOffset>
      <JapaneseText>御両親の命日も近いんだろ？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109501</PointerOffset>
      <JapaneseText>君がこのままこの連中と馬車を襲ったら、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109528</PointerOffset>
      <JapaneseText>アミィちゃんが悲しむ。それでもいいのか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109551</PointerOffset>
      <JapaneseText>チェスター、アミィちゃんが心配してたよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x10957a</PointerOffset>
      <JapaneseText>御両親の命日も近いんだろ？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109595</PointerOffset>
      <JapaneseText>「夜光花」を摘みに行きたい、って</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1095b6</PointerOffset>
      <JapaneseText>いってた。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1095c1</PointerOffset>
      <JapaneseText>でも、ここでこいつらの仲間になったら、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1095e8</PointerOffset>
      <JapaneseText>もうそんなこともできないぞ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109605</PointerOffset>
      <JapaneseText>それでもいいのか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109618</PointerOffset>
      <JapaneseText>よくねえよ！　そんなことは、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109635</PointerOffset>
      <JapaneseText>わかってんだよ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109646</PointerOffset>
      <JapaneseText>だがな、どうしろってんだよ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109663</PointerOffset>
      <JapaneseText>この人数だぞ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109672</PointerOffset>
      <JapaneseText>大したことないだろ？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109687</PointerOffset>
      <JapaneseText>……僕と、君の二人なら。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1096a1</PointerOffset>
      <JapaneseText>……へ、へへへ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1096b2</PointerOffset>
      <JapaneseText>ばかやろうだよ、おまえは！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1096cd</PointerOffset>
      <JapaneseText>悪いな、クレイトンさん。そういうわけだ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1096f6</PointerOffset>
      <JapaneseText>俺は、抜けさせてもらうぜ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109711</PointerOffset>
      <JapaneseText>あ、それから、馬車も襲わせねえから。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109736</PointerOffset>
      <JapaneseText>ちっ……やっぱりガキはガキか。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109755</PointerOffset>
      <JapaneseText>……かまわねえ！　てめえら、景気づけだ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x10977e</PointerOffset>
      <JapaneseText>このガキどもをぶっ殺せ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x109797</PointerOffset>
      <JapaneseText>おおっー！！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
