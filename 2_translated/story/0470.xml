
<SceneText>
  <FriendlyName>Conversation_0470</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x16d957</PointerOffset>
      <JapaneseText>そう。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16d95e</PointerOffset>
      <JapaneseText>あなた、面白いものを持っているわね。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16d983</PointerOffset>
      <JapaneseText>お初にお目にかかります。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16d99c</PointerOffset>
      <JapaneseText>偉大なる氷の精霊よ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16d9b1</PointerOffset>
      <JapaneseText>晶霊よ。この世界では、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16d9c8</PointerOffset>
      <JapaneseText>我らはそう呼ばれている。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16d9e1</PointerOffset>
      <JapaneseText>失礼いたしました、晶霊よ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16d9fc</PointerOffset>
      <JapaneseText>質問をひとつ、よろしいでしょうか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16da1f</PointerOffset>
      <JapaneseText>許します。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16da2a</PointerOffset>
      <JapaneseText>私は自分の身を再生する術を求め、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16da4b</PointerOffset>
      <JapaneseText>異世界よりやってまいりました。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16da6a</PointerOffset>
      <JapaneseText>この地のどこかに、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16da7d</PointerOffset>
      <JapaneseText>私の探す金属があるはずなのです。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16da9e</PointerOffset>
      <JapaneseText>ご存知であれば、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16daaf</PointerOffset>
      <JapaneseText>どうか教えていただきたい。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dacd</PointerOffset>
      <JapaneseText>ここより対面にあるところ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dae8</PointerOffset>
      <JapaneseText>レグルスの丘と呼ばれる場所へ行きなさい。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16db11</PointerOffset>
      <JapaneseText>詳しくは、そこのファラに聞くといい。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16db36</PointerOffset>
      <JapaneseText>そこに、あなたの求めるものがあります。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16db5d</PointerOffset>
      <JapaneseText>レグルスの丘ですね。わかりました。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16db80</PointerOffset>
      <JapaneseText>感謝いたします。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16db91</PointerOffset>
      <JapaneseText>だが、たとえ金属を手に入れたとしても、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dbb8</PointerOffset>
      <JapaneseText>この世界で剣に加工することはできません。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dbe1</PointerOffset>
      <JapaneseText>あなたはさらに、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dbf2</PointerOffset>
      <JapaneseText>別の世界をおとなう必要があります。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dc15</PointerOffset>
      <JapaneseText>旅の終えんは、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dc24</PointerOffset>
      <JapaneseText>すぐには訪れないと知りなさい。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dc43</PointerOffset>
      <JapaneseText>ご助言、しかと心に刻みます。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dc60</PointerOffset>
      <JapaneseText>氷の晶霊よ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dc73</PointerOffset>
      <JapaneseText>ふゆトマトが手に入って、よかったですね！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dc9c</PointerOffset>
      <JapaneseText>あなたのおかげよ。ありがと。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dcb9</PointerOffset>
      <JapaneseText>これでまったく同じ材料を使って、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dcda</PointerOffset>
      <JapaneseText>勝負することになりますね。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dcf5</PointerOffset>
      <JapaneseText>ええ。あとは純粋に、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dd0a</PointerOffset>
      <JapaneseText>お互いの技を競い合うのみ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dd25</PointerOffset>
      <JapaneseText>わたし、負けませんよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dd3c</PointerOffset>
      <JapaneseText>こちらこそ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dd49</PointerOffset>
      <JapaneseText>全力でお相手させていただくわ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dd68</PointerOffset>
      <JapaneseText>スポ根やるのは勝手だけどよ……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16dd89</PointerOffset>
      <JapaneseText>何もここじゃなくてもいいだろ？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
