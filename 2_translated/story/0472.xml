
<SceneText>
  <FriendlyName>Conversation_0472</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x16f7c8</PointerOffset>
      <JapaneseText>リーネの村</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f7d6</PointerOffset>
      <JapaneseText>ボブおじさーん。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f7e7</PointerOffset>
      <JapaneseText>おお、リリス。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f7f6</PointerOffset>
      <JapaneseText>トマト、なったんだって？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f80f</PointerOffset>
      <JapaneseText>おことばに甘えて、もらいにきちゃった。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f836</PointerOffset>
      <JapaneseText>そうかそうか。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f845</PointerOffset>
      <JapaneseText>好きなだけ、もっていくといい。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f864</PointerOffset>
      <JapaneseText>リリスはいつも喜んでくれるから、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f885</PointerOffset>
      <JapaneseText>あげがいがあるよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f898</PointerOffset>
      <JapaneseText>だってあたし、おじさんのトマト、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f8b9</PointerOffset>
      <JapaneseText>ほんとに大好きなんだもの！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f8d4</PointerOffset>
      <JapaneseText>おいしいトマトソースを作って、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f8f3</PointerOffset>
      <JapaneseText>お返しするからね。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f906</PointerOffset>
      <JapaneseText>楽しみにしているよ。わしもマギーも、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f92b</PointerOffset>
      <JapaneseText>リリスの料理のファンだからのう。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f94c</PointerOffset>
      <JapaneseText>まっかせといてよ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f95f</PointerOffset>
      <JapaneseText>ほっほっほ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f96c</PointerOffset>
      <JapaneseText>よし！　おじさんのトマトさえあれば、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f991</PointerOffset>
      <JapaneseText>きっと最高のペスカトーレが作れるわ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f9b6</PointerOffset>
      <JapaneseText>自信たっぷりだな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f9c9</PointerOffset>
      <JapaneseText>おじさんの作ったものにかなうトマトなんて</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16f9f2</PointerOffset>
      <JapaneseText>異世界探したってあるはずないわ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16fa13</PointerOffset>
      <JapaneseText>ディムロスにも食べさせてあげたいよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16fa38</PointerOffset>
      <JapaneseText>きっと驚くこと、間違いなしだよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16fa59</PointerOffset>
      <JapaneseText>ま、わたしには口がないからな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16fa7b</PointerOffset>
      <JapaneseText>もう一度あの異世界へ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x16fa92</PointerOffset>
      <JapaneseText>よろしくね、ディムロス。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
