
<SceneText>
  <FriendlyName>Conversation_0474</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x178665</PointerOffset>
      <JapaneseText>はい！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x17866c</PointerOffset>
      <JapaneseText>それでは！　ペスカトーレ対決、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x17868b</PointerOffset>
      <JapaneseText>始めさせていただきます！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1786a4</PointerOffset>
      <JapaneseText>ゲストの方に、お話をうかがいます。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1786c7</PointerOffset>
      <JapaneseText>なぜ、昔の味でなければならないのですか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1786f0</PointerOffset>
      <JapaneseText>あの女、すげえな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178703</PointerOffset>
      <JapaneseText>あんな速いみじん切り、見たことねえぞ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x17872a</PointerOffset>
      <JapaneseText>ファラも負けていないぞ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178743</PointerOffset>
      <JapaneseText>気合いの入りかたは、今までで一番だ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178768</PointerOffset>
      <JapaneseText>それでいて、動きにはまったくムダがない。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178791</PointerOffset>
      <JapaneseText>舞うがごとき、優雅な包丁さばきだ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1787b4</PointerOffset>
      <JapaneseText>のっけから、目まぐるしい攻防だぜ……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1787de</PointerOffset>
      <JapaneseText>まず「ミルク」をいためる</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1787f8</PointerOffset>
      <JapaneseText>まず「たまねぎ」をいためる</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178814</PointerOffset>
      <JapaneseText>まず「レタス」をいためる</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178831</PointerOffset>
      <JapaneseText>ペスカトーレには、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178844</PointerOffset>
      <JapaneseText>どのような思い出があるのですか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178868</PointerOffset>
      <JapaneseText>デバッグ「セルシウス」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178880</PointerOffset>
      <JapaneseText>デバッグ「ボブおじさん」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x17889a</PointerOffset>
      <JapaneseText>デバッグ「食材店のトマト」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1788b6</PointerOffset>
      <JapaneseText>次は「レモン」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1788c6</PointerOffset>
      <JapaneseText>次は「ボブおじさんのトマト」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1788e4</PointerOffset>
      <JapaneseText>次は「ベアのにく」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1788f8</PointerOffset>
      <JapaneseText>次は「レモン」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178908</PointerOffset>
      <JapaneseText>次は「ふゆトマト」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x17891c</PointerOffset>
      <JapaneseText>次は「ベアのにく」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178930</PointerOffset>
      <JapaneseText>次は「レモン」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178940</PointerOffset>
      <JapaneseText>次は「トマト」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178950</PointerOffset>
      <JapaneseText>次は「ベアのにく」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178967</PointerOffset>
      <JapaneseText>ハイレベルな戦いが繰り広げられています！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178990</PointerOffset>
      <JapaneseText>両選手とも、まったく退く構えなし。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1789b3</PointerOffset>
      <JapaneseText>これは、すごい。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1789c4</PointerOffset>
      <JapaneseText>間違いなく、歴史に残る一戦だ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1789e3</PointerOffset>
      <JapaneseText>どっちが作る料理も、うめーんだろうな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178a0a</PointerOffset>
      <JapaneseText>想像しただけで、腹が鳴りそうだ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178a2e</PointerOffset>
      <JapaneseText>さらに加えるのは「パスタ」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178a4a</PointerOffset>
      <JapaneseText>さらに加えるのは「ライス」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178a66</PointerOffset>
      <JapaneseText>さらに加えるのは「ブレッド」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178a87</PointerOffset>
      <JapaneseText>キール、気づいたか？　いつのまにか、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x178aac</PointerOffset>
      <JapaneseText>会場内がしんと静まり返っている。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
