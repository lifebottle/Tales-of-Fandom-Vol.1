
<SceneText>
  <FriendlyName>Conversation_0533</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x1a080e</PointerOffset>
      <JapaneseText>えへへ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0817</PointerOffset>
      <JapaneseText>感謝する。よくやってくれた、リリス。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a083c</PointerOffset>
      <JapaneseText>返事してくれないときは、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0855</PointerOffset>
      <JapaneseText>どうなることかと思ったよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0870</PointerOffset>
      <JapaneseText>さっき、全エネルギーを放射するとか、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0895</PointerOffset>
      <JapaneseText>いってたよね。あれって……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a08b2</PointerOffset>
      <JapaneseText>人間でいうなら、生命力に等しいな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a08d5</PointerOffset>
      <JapaneseText>あの後のわたしは、死んだも同然だった。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a08fc</PointerOffset>
      <JapaneseText>お前がこうして復活させてくれなかったら、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0925</PointerOffset>
      <JapaneseText>ずっとそのままだ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0938</PointerOffset>
      <JapaneseText>やっぱり……もう！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a094b</PointerOffset>
      <JapaneseText>なんでそんな危ないことするのよ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a096c</PointerOffset>
      <JapaneseText>お前ならきっと何とかしてくれると、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a098f</PointerOffset>
      <JapaneseText>信じていたからだ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a09a2</PointerOffset>
      <JapaneseText>そうだろう？　パートナーよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a09bf</PointerOffset>
      <JapaneseText>……ばか。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a09ca</PointerOffset>
      <JapaneseText>あの口ぶりだと、うまくいったようだな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a09f1</PointerOffset>
      <JapaneseText>よかった……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0a00</PointerOffset>
      <JapaneseText>ありがとうございます、クラースさん。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0a25</PointerOffset>
      <JapaneseText>クレスさんも。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0a34</PointerOffset>
      <JapaneseText>リリス！　そこにいるのは、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0a4f</PointerOffset>
      <JapaneseText>先ほど戦っていた剣士ではないか！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0a70</PointerOffset>
      <JapaneseText>クレスさんよ。隣にいるのはクラースさん。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0a99</PointerOffset>
      <JapaneseText>クラースさんがオリジンを呼んでくれたの。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0ac2</PointerOffset>
      <JapaneseText>オリジン？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0acd</PointerOffset>
      <JapaneseText>精霊だよ！　オリジンが、ディムロスと、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0af4</PointerOffset>
      <JapaneseText>リバヴィウス鉱を融合してくれたんだよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0b1b</PointerOffset>
      <JapaneseText>そうだったのか……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0b30</PointerOffset>
      <JapaneseText>剣をきたえる精霊を探してるって聞いて、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0b57</PointerOffset>
      <JapaneseText>すぐオリジンのことだとわかったよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0b7a</PointerOffset>
      <JapaneseText>わたしたちに出会えて、よかったな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0b9d</PointerOffset>
      <JapaneseText>リリスさん、申し訳ありませんでした。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0bc2</PointerOffset>
      <JapaneseText>なんておわびをしていいか。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0bdd</PointerOffset>
      <JapaneseText>いいんですよ。クレスさんたちのおかげで、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0c06</PointerOffset>
      <JapaneseText>ディムロスも元通りになったんですから。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0c2d</PointerOffset>
      <JapaneseText>終わりよければ、すべてよしです。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0c4e</PointerOffset>
      <JapaneseText>久しぶりに暴れて、スカッとできましたし。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0c77</PointerOffset>
      <JapaneseText>そういってもらえると、気が楽になります。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0ca0</PointerOffset>
      <JapaneseText>元はといえばいきなり空中に現われた、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0cc5</PointerOffset>
      <JapaneseText>我々のほうが悪いのだ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0cdc</PointerOffset>
      <JapaneseText>あれで怪しまれないはずがない。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0cfb</PointerOffset>
      <JapaneseText>クレスどのの行動は、当然だ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0d18</PointerOffset>
      <JapaneseText>我々、じゃないでしょ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0d2f</PointerOffset>
      <JapaneseText>いいや、連帯責任だ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0d44</PointerOffset>
      <JapaneseText>なぜならわたしたちは……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0d5f</PointerOffset>
      <JapaneseText>あーはいはい、パートナーだってんでしょ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0d88</PointerOffset>
      <JapaneseText>あんた、直るなり減らず口ばっかね。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0dab</PointerOffset>
      <JapaneseText>それで？　これからどうする？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0dc8</PointerOffset>
      <JapaneseText>すぐに、元の世界へ戻るのか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0de5</PointerOffset>
      <JapaneseText>そうですね。そろそろお夕飯のしたく、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0e0a</PointerOffset>
      <JapaneseText>しなくちゃいけないし。帰ります。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0e2b</PointerOffset>
      <JapaneseText>そうか。なら仕方ないな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0e44</PointerOffset>
      <JapaneseText>異世界の話を、もっと聞きたかったが。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0e69</PointerOffset>
      <JapaneseText>リリスさん。もし僕の力が必要なら、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0e8c</PointerOffset>
      <JapaneseText>いつでもいってください。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0ea5</PointerOffset>
      <JapaneseText>時空を飛び越えて、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0eb8</PointerOffset>
      <JapaneseText>必ず助けにいきますから！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0ed1</PointerOffset>
      <JapaneseText>ありがと、クレスさん。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0ee8</PointerOffset>
      <JapaneseText>そのときはよろしくね。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0eff</PointerOffset>
      <JapaneseText>この世界の人々に、幸あらんことを。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0f22</PointerOffset>
      <JapaneseText>ディムロスが、みなさんによろしくって。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0f49</PointerOffset>
      <JapaneseText>それじゃ、あたしたち、行きます。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0f6a</PointerOffset>
      <JapaneseText>本当にどうも、ありがとうございました。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0f97</PointerOffset>
      <JapaneseText>スカッとした、か。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0faa</PointerOffset>
      <JapaneseText>あの子にはかなわないな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0fc3</PointerOffset>
      <JapaneseText>いい体験ができたと思えばいい。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0fe2</PointerOffset>
      <JapaneseText>そうだろう？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a0fef</PointerOffset>
      <JapaneseText>ええ。今日のことを反省材料に、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a100e</PointerOffset>
      <JapaneseText>また自分を鍛え直します。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1027</PointerOffset>
      <JapaneseText>頑張れよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
