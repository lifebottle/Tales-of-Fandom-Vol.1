
<SceneText>
  <FriendlyName>Conversation_0537</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x1a19fb</PointerOffset>
      <JapaneseText>リーネの村</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1a08</PointerOffset>
      <JapaneseText>うございます、クラースさん。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1a25</PointerOffset>
      <JapaneseText>クレスさんも。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1a34</PointerOffset>
      <JapaneseText>リリス！　そこにいるのは、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1a4f</PointerOffset>
      <JapaneseText>先ほど戦っていた剣士ではないか！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1a70</PointerOffset>
      <JapaneseText>クレスさんよ。隣にいるのはクラースさん。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1a99</PointerOffset>
      <JapaneseText>クラースさんがオリジンを呼んでくれたの。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1ac2</PointerOffset>
      <JapaneseText>オリジン？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1acd</PointerOffset>
      <JapaneseText>精霊だよ！　オリジンが、ディムロスと、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1af4</PointerOffset>
      <JapaneseText>リバヴィウス鉱を融合してくれたんだよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1b1b</PointerOffset>
      <JapaneseText>そうだったのか……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1b30</PointerOffset>
      <JapaneseText>剣をきたえる精霊を探してるって聞いて、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1b57</PointerOffset>
      <JapaneseText>すぐオリジンのことだとわかったよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1b7a</PointerOffset>
      <JapaneseText>わたしたちに出会えて、よかったな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1b9d</PointerOffset>
      <JapaneseText>リリスさん、申し訳ありませんでした。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1bc2</PointerOffset>
      <JapaneseText>なんておわびをしていいか。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1bdd</PointerOffset>
      <JapaneseText>いいんですよ。クレスさんたちのおかげで、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1c06</PointerOffset>
      <JapaneseText>ディムロスも元通りになったんですから。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1c2d</PointerOffset>
      <JapaneseText>終わりよければ、すべてよしです。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1c4e</PointerOffset>
      <JapaneseText>久しぶりに暴れて、スカッとできましたし。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1c77</PointerOffset>
      <JapaneseText>そういってもらえると、気が楽になります。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1ca0</PointerOffset>
      <JapaneseText>元はといえばいきなり空中に現われた、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1cc5</PointerOffset>
      <JapaneseText>我々のほうが悪いのだ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1cdc</PointerOffset>
      <JapaneseText>あれで怪しまれないはずがない。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1cfb</PointerOffset>
      <JapaneseText>クレスどのの行動は、当然だ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1d18</PointerOffset>
      <JapaneseText>我々、じゃないでしょ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1d2f</PointerOffset>
      <JapaneseText>いいや、連帯責任だ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1d44</PointerOffset>
      <JapaneseText>なぜならわたしたちは……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1d5f</PointerOffset>
      <JapaneseText>あーはいはい、パートナーだってんでしょ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1d88</PointerOffset>
      <JapaneseText>あんた、直るなり減らず口ばっかね。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1dab</PointerOffset>
      <JapaneseText>それで？　これからどうする？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1dc8</PointerOffset>
      <JapaneseText>すぐに、元の世界へ戻るのか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1de5</PointerOffset>
      <JapaneseText>そうですね。そろそろお夕飯のしたく、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1e0a</PointerOffset>
      <JapaneseText>しなくちゃいけないし。帰ります。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1e2b</PointerOffset>
      <JapaneseText>そうか。なら仕方ないな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1e44</PointerOffset>
      <JapaneseText>異世界の話を、もっと聞きたかったが。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1e69</PointerOffset>
      <JapaneseText>リリスさん。もし僕の力が必要なら、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1e8c</PointerOffset>
      <JapaneseText>いつでもいってください。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1ea5</PointerOffset>
      <JapaneseText>時空を飛び越えて、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1eb8</PointerOffset>
      <JapaneseText>必ず助けにいきますから！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1ed1</PointerOffset>
      <JapaneseText>ありがと、クレスさん。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1ee8</PointerOffset>
      <JapaneseText>そのときはよろしくね。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1eff</PointerOffset>
      <JapaneseText>この世界の人々に、幸あらんことを。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1f22</PointerOffset>
      <JapaneseText>ディムロスが、みなさんによろしくって。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1f49</PointerOffset>
      <JapaneseText>それじゃ、あたしたち、行きます。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1f6a</PointerOffset>
      <JapaneseText>本当にどうも、ありがとうございました。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1f97</PointerOffset>
      <JapaneseText>スカッとした、か。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1faa</PointerOffset>
      <JapaneseText>あの子にはかなわないな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1fc3</PointerOffset>
      <JapaneseText>いい体験ができたと思えばいい。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1fe2</PointerOffset>
      <JapaneseText>そうだろう？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1a1fef</PointerOffset>
      <JapaneseText>ええ。今日のこと</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
