
<SceneText>
  <FriendlyName>Conversation_0578</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x1bd178</PointerOffset>
      <JapaneseText>名探偵ポイント</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd18a</PointerOffset>
      <JapaneseText>犯人は私たちに「ゲーム」を挑んだ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd1ae</PointerOffset>
      <JapaneseText>つまり、「ルール」とも呼ぶべき法則が</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd1d3</PointerOffset>
      <JapaneseText>必ずどこかに隠されているはず！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd1f4</PointerOffset>
      <JapaneseText>たとえば……あの壁のカレンダー！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd216</PointerOffset>
      <JapaneseText>カレンダーが可憐だー、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd22d</PointerOffset>
      <JapaneseText>なんちゃってー……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd245</PointerOffset>
      <JapaneseText>……もっとセンスのいい</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd25c</PointerOffset>
      <JapaneseText>ダジャレが聞きたいね。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd279</PointerOffset>
      <JapaneseText>このライン、赤いわね……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
