
<SceneText>
  <FriendlyName>Conversation_0586</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x1bd6f1</PointerOffset>
      <JapaneseText>キール？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd6fa</PointerOffset>
      <JapaneseText>それって……どういう……？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd717</PointerOffset>
      <JapaneseText>犯人――「姿なき晶霊術士」が</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd734</PointerOffset>
      <JapaneseText>ゲームを挑んでいたのはこの僕だ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd756</PointerOffset>
      <JapaneseText>ところが今回、僕は推理を途中で放棄し、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd77d</PointerOffset>
      <JapaneseText>判断を他人に預けてしまった。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd79c</PointerOffset>
      <JapaneseText>ゲームのプレイヤーが変わるという</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd7bd</PointerOffset>
      <JapaneseText>予想外の展開に、犯人はボロを出した。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd7e3</PointerOffset>
      <JapaneseText>……そうだな、ヒューラ？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd7fe</PointerOffset>
      <JapaneseText>くっ……！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd80b</PointerOffset>
      <JapaneseText>ま、まさか！？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd81d</PointerOffset>
      <JapaneseText>ヒューラ・ヴェルミリオン。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd838</PointerOffset>
      <JapaneseText>「姿なき晶霊術士」は……君だッ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
