
<SceneText>
  <FriendlyName>Conversation_0587</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x1bd85a</PointerOffset>
      <JapaneseText>動くな！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd867</PointerOffset>
      <JapaneseText>ヒューラ・ヴェルミリオン、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd882</PointerOffset>
      <JapaneseText>時計塔爆破の容疑で逮捕する！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd8a1</PointerOffset>
      <JapaneseText>……ふっ。どうやら私の負けらしいな、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd8c6</PointerOffset>
      <JapaneseText>キール・ツァイベル。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd8de</PointerOffset>
      <JapaneseText>ど、どうして……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd8f4</PointerOffset>
      <JapaneseText>ミンツいちの名探偵のあなたが……？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd918</PointerOffset>
      <JapaneseText>若き名探偵の噂を聞いてね。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd935</PointerOffset>
      <JapaneseText>試してみたくなった。私か、お前か。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd958</PointerOffset>
      <JapaneseText>どちらの頭脳が上なのかを、な。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd979</PointerOffset>
      <JapaneseText>なんてヤツなの！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd98d</PointerOffset>
      <JapaneseText>人生はゲームだ。それに勝利する事が</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd9b0</PointerOffset>
      <JapaneseText>私の生きる意味なのだよ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd9cb</PointerOffset>
      <JapaneseText>ふふふ。キール。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd9dc</PointerOffset>
      <JapaneseText>これで終わりと思うな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bd9f6</PointerOffset>
      <JapaneseText>いつか、おまえより私が優れている事を</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bda1b</PointerOffset>
      <JapaneseText>証明してみせる。ふっふっふ……</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bda3c</PointerOffset>
      <JapaneseText>はーっはっはっは！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bda52</PointerOffset>
      <JapaneseText>どもーっ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bda5d</PointerOffset>
      <JapaneseText>新聞部のルナリアです！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bda77</PointerOffset>
      <JapaneseText>例の「姿なき晶霊術士」事件、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bda94</PointerOffset>
      <JapaneseText>いま解決したんですよね！？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bdab1</PointerOffset>
      <JapaneseText>詳しいおはなしを聞かせてください！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bdad5</PointerOffset>
      <JapaneseText>ああ……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bdae0</PointerOffset>
      <JapaneseText>探偵は僕じゃない。そっちだ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bdaff</PointerOffset>
      <JapaneseText>またまたお手柄ですね、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bdb16</PointerOffset>
      <JapaneseText>プリムラさん！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bdb29</PointerOffset>
      <JapaneseText>あー……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bdb36</PointerOffset>
      <JapaneseText>ま、まあ、余裕ですよ、今回も！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bdb57</PointerOffset>
      <JapaneseText>要するにアレです。その……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bdb74</PointerOffset>
      <JapaneseText>真実は常にひとつ？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1bdb8a</PointerOffset>
      <JapaneseText>あは、あははははははは！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
