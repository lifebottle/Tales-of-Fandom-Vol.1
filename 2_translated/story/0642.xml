
<SceneText>
  <FriendlyName>Conversation_0642</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x1dfb7c</PointerOffset>
      <JapaneseText>うう〜。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfb89</PointerOffset>
      <JapaneseText>キールなら罪をなすりつけやすい</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfba9</PointerOffset>
      <JapaneseText>キールを恨んでいる</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfbbe</PointerOffset>
      <JapaneseText>わからない</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfbca</PointerOffset>
      <JapaneseText>う〜……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfbd6</PointerOffset>
      <JapaneseText>いまの段階じゃわからないよ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfbf5</PointerOffset>
      <JapaneseText>そうだ。それが正解だ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfc0f</PointerOffset>
      <JapaneseText>なぜキールがターゲットにされたのかは、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfc36</PointerOffset>
      <JapaneseText>いまの段階では推理不可能だ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfc55</PointerOffset>
      <JapaneseText>ムムッ！？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfc61</PointerOffset>
      <JapaneseText>もしかして私を試したの！？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfc7e</PointerOffset>
      <JapaneseText>フフ……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfc89</PointerOffset>
      <JapaneseText>では、別の側面から状況を攻めてみよう。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfcb1</PointerOffset>
      <JapaneseText>盗まれたのは特級学士認定試験問題。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfcd4</PointerOffset>
      <JapaneseText>こっちはハッキリしている。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfcf1</PointerOffset>
      <JapaneseText>試験問題を盗む目的はただひとつ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfd12</PointerOffset>
      <JapaneseText>わかるな……？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfd25</PointerOffset>
      <JapaneseText>試験でいい点数を取るため</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfd3f</PointerOffset>
      <JapaneseText>キールに罪を被せるため</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfd57</PointerOffset>
      <JapaneseText>キールに罪を被せるため？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfd72</PointerOffset>
      <JapaneseText>だったら、もっと簡単な方法がある。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfd96</PointerOffset>
      <JapaneseText>なにも警戒厳重な部屋から試験問題を</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfdb9</PointerOffset>
      <JapaneseText>盗み出す危険を犯す必要はない。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfdda</PointerOffset>
      <JapaneseText>でも退学させるくらいの</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfdf1</PointerOffset>
      <JapaneseText>大きな罪じゃないと――</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfe0b</PointerOffset>
      <JapaneseText>私だったら、怪しげな女に金を握らせて、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfe32</PointerOffset>
      <JapaneseText>キールに弄ばれたと訴え出させるがね。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfe58</PointerOffset>
      <JapaneseText>格式を重んじるミンツ大学、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfe73</PointerOffset>
      <JapaneseText>放校になるのは難しいことじゃない。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfe97</PointerOffset>
      <JapaneseText>うう〜……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfea8</PointerOffset>
      <JapaneseText>試験問題を盗む目的は……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfec5</PointerOffset>
      <JapaneseText>あらかじめ問題がわかっていれば、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1dfee6</PointerOffset>
      <JapaneseText>試験でいい点数を取れるからよね。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
