
<SceneText>
  <FriendlyName>Conversation_0662</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x1eb883</PointerOffset>
      <JapaneseText>あ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eb888</PointerOffset>
      <JapaneseText>クレーメルケージがついてる……？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eb8aa</PointerOffset>
      <JapaneseText>いいところに目をつけたね！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eb8c7</PointerOffset>
      <JapaneseText>風晶霊の力を借りて、これを持つ者同士</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eb8ec</PointerOffset>
      <JapaneseText>の遠隔会話を可能にするんだ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eb90b</PointerOffset>
      <JapaneseText>名づけて、「クレーメルトーク」！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eb92d</PointerOffset>
      <JapaneseText>すごーい！　サンクってば天才！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eb94e</PointerOffset>
      <JapaneseText>へへへ。まあ、残念ながらアンテナがある</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eb975</PointerOffset>
      <JapaneseText>この部屋の中でしか使えないし、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eb995</PointerOffset>
      <JapaneseText>時々聞こえなくなったりもするし、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eb9b6</PointerOffset>
      <JapaneseText>その……軽く爆発したりね、ウン。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eb9d8</PointerOffset>
      <JapaneseText>爆発〜！？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eb9e7</PointerOffset>
      <JapaneseText>時々だよ時々！　乱暴に扱うと、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eba06</PointerOffset>
      <JapaneseText>ごくまれに煙が出たり。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eba20</PointerOffset>
      <JapaneseText>安心してよ、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eba2d</PointerOffset>
      <JapaneseText>ちょっと火傷するくらいの爆発だから。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eba53</PointerOffset>
      <JapaneseText>……いらない。返す。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eba6b</PointerOffset>
      <JapaneseText>まあまあまあまあ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1eba81</PointerOffset>
      <JapaneseText>これは試作第ニ号でね、一号機は</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1ebaa0</PointerOffset>
      <JapaneseText>キールに渡してあるんだ。つまり――</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1ebac4</PointerOffset>
      <JapaneseText>キールと会話ができるってこと！？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
