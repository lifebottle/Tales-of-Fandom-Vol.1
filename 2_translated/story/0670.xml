
<SceneText>
  <FriendlyName>Conversation_0670</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x1f0520</PointerOffset>
      <JapaneseText>なんだい、君は？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0531</PointerOffset>
      <JapaneseText>勉強の邪魔だ、帰ってくれ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f054e</PointerOffset>
      <JapaneseText>黒体の異変は吉兆だというのが</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f056b</PointerOffset>
      <JapaneseText>セイファート教会の公式見解だけど……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0593</PointerOffset>
      <JapaneseText>僕はちょっと変だと思うんだ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f05b2</PointerOffset>
      <JapaneseText>まあ、表立ってケチつけるほど</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f05cf</PointerOffset>
      <JapaneseText>バカじゃないけどね……</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f05e9</PointerOffset>
      <JapaneseText>…………。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f05f8</PointerOffset>
      <JapaneseText>噂だけど、試験問題が金で買えるらしいぜ？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0621</PointerOffset>
      <JapaneseText>誰に頼めばいいのか、わかったら僕にも</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0646</PointerOffset>
      <JapaneseText>教えてくれよ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0659</PointerOffset>
      <JapaneseText>…………。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0668</PointerOffset>
      <JapaneseText>ラシュアンを知っているかい？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0685</PointerOffset>
      <JapaneseText>メチャクチャ田舎なんだぜ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f06a2</PointerOffset>
      <JapaneseText>死ぬまでに一回でいいから</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f06bb</PointerOffset>
      <JapaneseText>大晶霊とあってみたいなぁ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f06d8</PointerOffset>
      <JapaneseText>君も大晶霊ウンディーネのファンクラブに</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f06ff</PointerOffset>
      <JapaneseText>入会しないか！？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0711</PointerOffset>
      <JapaneseText>イフリートのイメージカラーは赤！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0732</PointerOffset>
      <JapaneseText>試験に出るぞ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0742</PointerOffset>
      <JapaneseText>隣の部屋のヤツが犯人だよ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f075d</PointerOffset>
      <JapaneseText>昨日も遅くまで酒飲んで大騒ぎしてたし！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0785</PointerOffset>
      <JapaneseText>キール？　アイツ、感じ悪いよね……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f07ab</PointerOffset>
      <JapaneseText>…………。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f07ba</PointerOffset>
      <JapaneseText>試験勉強で徹夜続きでお肌がカサカサ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f07e0</PointerOffset>
      <JapaneseText>しっかり考えて行動すると、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f07fb</PointerOffset>
      <JapaneseText>名探偵ポイントがあがるらしいわよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f081f</PointerOffset>
      <JapaneseText>…………。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f082e</PointerOffset>
      <JapaneseText>伝説の生物ねこにん……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0847</PointerOffset>
      <JapaneseText>友達になりたい……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f085f</PointerOffset>
      <JapaneseText>適当に移動していると名探偵ポイントが</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0884</PointerOffset>
      <JapaneseText>下がっちゃうわよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f089a</PointerOffset>
      <JapaneseText>…………。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f08a9</PointerOffset>
      <JapaneseText>ウルタス・ブイの劇、観に行きたいなあ……</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f08d2</PointerOffset>
      <JapaneseText>適当に動き回るくらいなら</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f08eb</PointerOffset>
      <JapaneseText>アドバイスを仰いだほうが賢明ね</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f090c</PointerOffset>
      <JapaneseText>…………。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f091b</PointerOffset>
      <JapaneseText>大晶霊が主人公の小説を書いているの。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0941</PointerOffset>
      <JapaneseText>イフリート様は知性的で、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f095a</PointerOffset>
      <JapaneseText>ハンサムなお方なのよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0974</PointerOffset>
      <JapaneseText>ああ、イフリート様……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f098f</PointerOffset>
      <JapaneseText>王都から来るロエンって人、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f09aa</PointerOffset>
      <JapaneseText>アレンデ姫にぞっこんなんだって！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f09cc</PointerOffset>
      <JapaneseText>世界は謎に満ちている……ぶつぶつぶつ……</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f09f5</PointerOffset>
      <JapaneseText>あてずっぽうに動いても時間の無駄だわ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0a1d</PointerOffset>
      <JapaneseText>ちゃんと考えて行動しなくちゃ</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0a3a</PointerOffset>
      <JapaneseText>名探偵の名が泣くわ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0a52</PointerOffset>
      <JapaneseText>あてずっぽうに動いても無意味だわ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0a77</PointerOffset>
      <JapaneseText>ここはよーく考えて……考えて……</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0a98</PointerOffset>
      <JapaneseText>考えて……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0aa9</PointerOffset>
      <JapaneseText>プリムラ。僕は実は……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0ac5</PointerOffset>
      <JapaneseText>セレスティアから来た</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x1f0ada</PointerOffset>
      <JapaneseText>セレスティア王子なんだ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
