
<SceneText>
  <FriendlyName>Conversation_0767</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x232638</PointerOffset>
      <JapaneseText>名探偵ポイント</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232647</PointerOffset>
      <JapaneseText>君が持っている生徒一覧表で、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232664</PointerOffset>
      <JapaneseText>誰がどの部屋にいるのかは一目瞭然だ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x23268a</PointerOffset>
      <JapaneseText>それを見て行動するといいだろう。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2326ab</PointerOffset>
      <JapaneseText>頑張ってくれ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2326be</PointerOffset>
      <JapaneseText>「評議会」の名前を口にしていた男の名前は</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2326e7</PointerOffset>
      <JapaneseText>ジャッジだかジャッドだかって男だ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x23270b</PointerOffset>
      <JapaneseText>似たような名前の男の部屋に</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232726</PointerOffset>
      <JapaneseText>行ってみてくれ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x23273a</PointerOffset>
      <JapaneseText>四〇二号室にジャッコって名前の男が</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x23275d</PointerOffset>
      <JapaneseText>いるじゃないか。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232771</PointerOffset>
      <JapaneseText>しっかりしてくれよ、まったく！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232792</PointerOffset>
      <JapaneseText>僕の命運は君が握っているんだぞ！？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2327b6</PointerOffset>
      <JapaneseText>暗号か……。たぶん、イニシャルを示して</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2327dd</PointerOffset>
      <JapaneseText>いるんじゃないか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2327f3</PointerOffset>
      <JapaneseText>Ｗ・Ｗ、Ａ・Ｈ、Ａ・Ｌ、だったよな？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232819</PointerOffset>
      <JapaneseText>たとえば君の名前はプリムラ・ロッソだから</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232842</PointerOffset>
      <JapaneseText>イニシャルはＰ・Ｒ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x23285a</PointerOffset>
      <JapaneseText>僕の場合はＫ・Ｚになるというわけだな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232882</PointerOffset>
      <JapaneseText>君が持っている生徒一覧表と</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x23289d</PointerOffset>
      <JapaneseText>照らし合わせれば、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2328b3</PointerOffset>
      <JapaneseText>イニシャルから元の名前を</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2328cc</PointerOffset>
      <JapaneseText>割り出せるんじゃないかな……？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2328ed</PointerOffset>
      <JapaneseText>……。Ｗ・Ｗはウィリアム・ウォーベックの</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232916</PointerOffset>
      <JapaneseText>イニシャルだろう？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x23292c</PointerOffset>
      <JapaneseText>彼が住んでいるのは三〇二号室。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x23294b</PointerOffset>
      <JapaneseText>そういうふうに考えるだけの問題だろう！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232973</PointerOffset>
      <JapaneseText>君には本当に失望させられるよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232994</PointerOffset>
      <JapaneseText>どうやら僕も覚悟を決めたほうが</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2329b3</PointerOffset>
      <JapaneseText>良さそうだな……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2329c9</PointerOffset>
      <JapaneseText>う〜ん。新聞部新聞部……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2329e6</PointerOffset>
      <JapaneseText>プリムラ、君の知り合いに</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2329ff</PointerOffset>
      <JapaneseText>新聞部のやつはいないか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>32</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232a1a</PointerOffset>
      <JapaneseText>「姿なき晶霊術士事件」を解決した時に取材に来た女の子。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>33</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232a54</PointerOffset>
      <JapaneseText>取材の前に名乗ってなかったか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>34</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232a75</PointerOffset>
      <JapaneseText>ルナリアって名乗っていたよな、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>35</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232a94</PointerOffset>
      <JapaneseText>あの女の子。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>36</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232aa5</PointerOffset>
      <JapaneseText>風の宮殿に住んでるって……なんのことだ？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>37</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232ace</PointerOffset>
      <JapaneseText>シルフの君……シルフ……シルフ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>38</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232af0</PointerOffset>
      <JapaneseText>そうだ！　シルフに関係する部屋を</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>39</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232b11</PointerOffset>
      <JapaneseText>探してみたらどうだ！？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>40</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232b2b</PointerOffset>
      <JapaneseText>晶霊にはそれぞれイメージカラーがあるのは</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>41</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232b54</PointerOffset>
      <JapaneseText>知っているな？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>42</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232b67</PointerOffset>
      <JapaneseText>イフリートは赤。ウンディーネは水色。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>43</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232b8c</PointerOffset>
      <JapaneseText>シルフは……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>44</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232b9f</PointerOffset>
      <JapaneseText>学生寮は各階ごとに色分けされている。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>45</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232bc5</PointerOffset>
      <JapaneseText>シルフのイメージカラーである緑色の階に</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>46</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232bec</PointerOffset>
      <JapaneseText>住んでいる男。そいつが「シルフの君」だ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>47</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232c15</PointerOffset>
      <JapaneseText>僕たちの行為自体、ある意味神への挑戦とも</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>48</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232c3e</PointerOffset>
      <JapaneseText>いえるよな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>49</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232c4f</PointerOffset>
      <JapaneseText>「呪われし場所」というのは、単なる文章の</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>50</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232c78</PointerOffset>
      <JapaneseText>遊びだろう。あまり気にしないほうがいいな</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>51</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232ca1</PointerOffset>
      <JapaneseText>学問とはすなわち、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>52</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232cb7</PointerOffset>
      <JapaneseText>セイファートが定めた目に見えないルールを</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>53</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232ce0</PointerOffset>
      <JapaneseText>探り当てようとする試みだ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>54</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232cfd</PointerOffset>
      <JapaneseText>これを「神の御業に挑む」と</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>55</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232d18</PointerOffset>
      <JapaneseText>言っているんじゃないか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>56</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232d33</PointerOffset>
      <JapaneseText>行くべきところは教室しかないだろう、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>57</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232d58</PointerOffset>
      <JapaneseText>少しは自分で頭を働かせてくれ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>58</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232d79</PointerOffset>
      <JapaneseText>君には想像力というものが</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>59</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232d92</PointerOffset>
      <JapaneseText>欠落しているのか！？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>60</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232daa</PointerOffset>
      <JapaneseText>これは難しいな……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>61</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232dc2</PointerOffset>
      <JapaneseText>ルナリアの言葉のなかに、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>62</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232ddb</PointerOffset>
      <JapaneseText>ヒントがあるのかもしれない。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>63</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232dfa</PointerOffset>
      <JapaneseText>彼女が話した内容で、まだヒントになって</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>64</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232e21</PointerOffset>
      <JapaneseText>いないような部分がないか、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>65</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232e3e</PointerOffset>
      <JapaneseText>しっかり思い出してくれ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>66</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232e59</PointerOffset>
      <JapaneseText>それぞれの名前を</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>67</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232e6a</PointerOffset>
      <JapaneseText>部屋番号に入れ替えてみようか。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>68</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232e8b</PointerOffset>
      <JapaneseText>キーファは「一〇二」。アンは「一〇一」</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>69</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x232eb2</PointerOffset>
      <JapaneseText>……う〜ん、わからないな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>70</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
