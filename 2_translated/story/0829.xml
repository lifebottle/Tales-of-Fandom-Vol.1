
<SceneText>
  <FriendlyName>Conversation_0829</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x24f038</PointerOffset>
      <JapaneseText>いやーっ！　シッシ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f04d</PointerOffset>
      <JapaneseText>ちょっとあっちいきなさいよー！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f06e</PointerOffset>
      <JapaneseText>ワンワン！　ワンワン！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f085</PointerOffset>
      <JapaneseText>こっちだ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f090</PointerOffset>
      <JapaneseText>木の上にいるぞ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f0a4</PointerOffset>
      <JapaneseText>も、もしかしてメチャクチャ</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f0bf</PointerOffset>
      <JapaneseText>絶体絶命―っ！？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f0d4</PointerOffset>
      <JapaneseText>晶霊術を使う</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f0e2</PointerOffset>
      <JapaneseText>ニセ晶霊爆弾作戦で切り抜ける</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f100</PointerOffset>
      <JapaneseText>火晶霊よ、私に力を！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f115</PointerOffset>
      <JapaneseText>ファイアーボール！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f12b</PointerOffset>
      <JapaneseText>きゃーん！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f13a</PointerOffset>
      <JapaneseText>わーっはっは！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f149</PointerOffset>
      <JapaneseText>光晶霊術学士をなめてもらっちゃ……</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f16d</PointerOffset>
      <JapaneseText>あ……ああっ！？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f182</PointerOffset>
      <JapaneseText>…………。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f190</PointerOffset>
      <JapaneseText>火事だー！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f19b</PointerOffset>
      <JapaneseText>早く消火しろー！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f1af</PointerOffset>
      <JapaneseText>う、うわー！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f1bc</PointerOffset>
      <JapaneseText>た、助けてくれー！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f1d2</PointerOffset>
      <JapaneseText>わ、わざとじゃないからね？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f1ed</PointerOffset>
      <JapaneseText>わざとじゃないのよ……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f208</PointerOffset>
      <JapaneseText>あなたたち！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>22</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f215</PointerOffset>
      <JapaneseText>私に近づくとこの晶霊爆弾を――！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>23</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f237</PointerOffset>
      <JapaneseText>…………。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>24</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f246</PointerOffset>
      <JapaneseText>脅しじゃないわよ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>25</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f259</PointerOffset>
      <JapaneseText>半径３０００ランゲが吹っ飛ぶ――</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>26</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f27b</PointerOffset>
      <JapaneseText>残念だったな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>27</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f28e</PointerOffset>
      <JapaneseText>お前さんの一発ギャグは</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>28</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f2a5</PointerOffset>
      <JapaneseText>もうネタバレなんだよ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>29</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f2c2</PointerOffset>
      <JapaneseText>縛って倉庫に転がしといたのに！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>30</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x24f2e3</PointerOffset>
      <JapaneseText>覚悟しろよ、女ァ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>31</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
