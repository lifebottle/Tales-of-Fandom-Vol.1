
<SceneText>
  <FriendlyName>Conversation_0949</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x28fd4d</PointerOffset>
      <JapaneseText>なに？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28fd54</PointerOffset>
      <JapaneseText>……「真の主役になる方法」？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28fd73</PointerOffset>
      <JapaneseText>そのとおり！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28fd80</PointerOffset>
      <JapaneseText>私ふくめて君達みんなカッコイイ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28fda2</PointerOffset>
      <JapaneseText>恐ろしく鈍いヤツ！　寝起きの悪いヤツ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28fdc9</PointerOffset>
      <JapaneseText>食い意地の張ったヤツ！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28fde3</PointerOffset>
      <JapaneseText>そんな困ったヤツらに主人公を</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28fe00</PointerOffset>
      <JapaneseText>まかせておくのは悔しいじゃないか！？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28fe26</PointerOffset>
      <JapaneseText>た、たしかにそうだな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28fe3d</PointerOffset>
      <JapaneseText>たまにはオレ達が主役になったっていいか……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28fe6a</PointerOffset>
      <JapaneseText>ふん、その方法とやらを聞こうじゃないか。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28fe93</PointerOffset>
      <JapaneseText>ではいくぞ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28fea3</PointerOffset>
      <JapaneseText>方法その１。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28feb0</PointerOffset>
      <JapaneseText>主人公は歌って踊れる芸人であるべし！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28fed6</PointerOffset>
      <JapaneseText>げ、芸人？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28fee1</PointerOffset>
      <JapaneseText>方法その２。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28feee</PointerOffset>
      <JapaneseText>主人公は衣装に命をかけるべし！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28ff0f</PointerOffset>
      <JapaneseText>なんか偏ってねぇか？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28ff27</PointerOffset>
      <JapaneseText>おい！　この本の著者は誰だ？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28ff46</PointerOffset>
      <JapaneseText>えーっと……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>19</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28ff55</PointerOffset>
      <JapaneseText>ジョニー＝シデンと書いてあるな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>20</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x28ff77</PointerOffset>
      <JapaneseText>やっぱり……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>21</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
