
<SceneText>
  <FriendlyName>Conversation_1051</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x2d81ac</PointerOffset>
      <JapaneseText>ふむ。かつてセインガルド王家御用達の</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2d81d1</PointerOffset>
      <JapaneseText>名酒と言われた『シャトー・ルエイン』</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2d81f6</PointerOffset>
      <JapaneseText>その象徴とも言える黄金の王冠ですからな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2d821f</PointerOffset>
      <JapaneseText>その価値５００万ガルドはくだりますまい。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2d8248</PointerOffset>
      <JapaneseText>う……そ……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2d8257</PointerOffset>
      <JapaneseText>だってそんなことなんにも……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2d8276</PointerOffset>
      <JapaneseText>ほっほっほ。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2d8283</PointerOffset>
      <JapaneseText>渡る世間はカモばかりですな。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x2d82a1</PointerOffset>
      <JapaneseText>それでは、ごきげんよう。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
