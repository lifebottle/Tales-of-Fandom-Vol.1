
<SceneText>
  <FriendlyName>Conversation_1210</FriendlyName>
  <Strings>
    <Section>Speaker</Section>
    <Entry>
      <PointerOffset>0x31528a</PointerOffset>
      <JapaneseText>……。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>0</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x315291</PointerOffset>
      <JapaneseText>よ〜し！　５ガルド発見！！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>1</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x3152ac</PointerOffset>
      <JapaneseText>……アレ？　どしたの、リッド？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>2</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x3152cb</PointerOffset>
      <JapaneseText>戦闘終わった？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>3</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x3152da</PointerOffset>
      <JapaneseText>……論外。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>4</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x31531b</PointerOffset>
      <JapaneseText>なあなあキール！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>5</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x31532c</PointerOffset>
      <JapaneseText>……どうした？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>6</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x31533b</PointerOffset>
      <JapaneseText>そんなに嬉しそうに？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>7</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x315350</PointerOffset>
      <JapaneseText>メルディな！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>8</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x31535d</PointerOffset>
      <JapaneseText>インフェリア語覚えたい！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>9</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x315376</PointerOffset>
      <JapaneseText>……だから、本を借りてきた！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>10</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x315393</PointerOffset>
      <JapaneseText>ふむ……、それはいいことだが、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>11</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x3153b2</PointerOffset>
      <JapaneseText>……何故僕のところに来たんだ？</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>12</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x3153d1</PointerOffset>
      <JapaneseText>う〜ん、本を借りたのはいいんだけど……、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>13</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x3153fa</PointerOffset>
      <JapaneseText>やっぱりメルディにはちょっと難しい。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>14</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x31541f</PointerOffset>
      <JapaneseText>だから、まずはキールに読んでもらって、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>15</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x315446</PointerOffset>
      <JapaneseText>その後にメルディが読む！</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>16</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x31545f</PointerOffset>
      <JapaneseText>そんなの……、</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>17</Id>
      <Status>To Do</Status>
    </Entry>
    <Entry>
      <PointerOffset>0x31546e</PointerOffset>
      <JapaneseText>ファラやリッドでもいいじゃないか。</JapaneseText>
      <EnglishText> </EnglishText>
      <Notes> </Notes>
      <Id>18</Id>
      <Status>To Do</Status>
    </Entry>
  </Strings>
  <Strings>
    <Section>system</Section>
  </Strings>
</SceneText>
